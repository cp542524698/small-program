.vLine {
  height: 2rpx;
  background-color: #e5e5e5;
}

.vDate {
  margin-left: 40rpx;
}

.vDate text {
  height: 48rpx;
  font-size: 34rpx;
  color: rgba(0, 0, 0, 0.54);
}

.vWorkRecord {
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.12);
  margin-left: 40rpx;
  height: 132rpx;
  display: flex;
  align-items: center;
}

.pWorkers-item {
  height: 86rpx;
  background-color: #fafafa;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.12);
}
.iGoIcon
{
    width: 24rpx;
    height: 24rpx;
}
.vWorkRecord image {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
}

.tName {
  height: 48rpx;
  font-size: 34rpx;
  margin-left: 20rpx;
}

.tWorkTime {
  font-size: 17px;
  color: #ffb74d;
  margin: 0rpx auto 0rpx auto;
}
.tNoWorkTime
{
  margin: 0rpx auto 0rpx auto;
}
